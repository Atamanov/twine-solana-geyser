        {
          "datasource": {
            "type": "postgres",
            "uid": "$timescale"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH ordered_slots AS (\n  SELECT slot,\n    CASE WHEN (\n      bank_hash = '' \n      OR bank_hash LIKE 'MISSING_%' \n      OR bank_hash LIKE 'PLACEHOLDER_%'\n      OR cumulative_lthash = '\\x00'\n      OR length(cumulative_lthash) = 0\n    ) THEN 1 ELSE 0 END as is_missing,\n    LAG(CASE WHEN (\n      bank_hash = '' \n      OR bank_hash LIKE 'MISSING_%' \n      OR bank_hash LIKE 'PLACEHOLDER_%'\n      OR cumulative_lthash = '\\x00'\n      OR length(cumulative_lthash) = 0\n    ) THEN 1 ELSE 0 END, 1, 0) OVER (ORDER BY slot) as prev_is_missing\n  FROM slots\n  WHERE status = 'rooted'\n)\nSELECT COUNT(*) as total_gaps\nFROM ordered_slots\nWHERE is_missing = 1 AND prev_is_missing = 0",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Total Gaps (Missing Data)",
      "type": "stat"
    },
