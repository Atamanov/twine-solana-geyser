      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "$timescale"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  slot,\n  status,\n  bank_hash,\n  parent_bank_hash,\n  signature_count,\n  last_blockhash,\n  LEFT(encode(delta_lthash, 'hex'), 32) || '...' as delta_lthash_hex,\n  LEFT(encode(cumulative_lthash, 'hex'), 32) || '...' as cumulative_lthash_hex,\n  accounts_delta_hash,\n  blockhash,\n  executed_transaction_count,\n  entry_count,\n  rooted_at\nFROM slots \nORDER BY slot DESC \nLIMIT 25",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Recent Slots with LtHashes (Hex)",
      "type": "table"
