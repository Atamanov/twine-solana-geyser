        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "editorMode": "code",
            "expr": "histogram_quantile(0.99, sum(rate(geyser_write_latency_microseconds_bucket[5m])) by (le, operation))",
            "legendFormat": "p99 {{operation}}",
            "range": true,
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "editorMode": "code",
            "expr": "histogram_quantile(0.95, sum(rate(geyser_write_latency_microseconds_bucket[5m])) by (le, operation))",
            "legendFormat": "p95 {{operation}}",
            "range": true,
            "refId": "B"
