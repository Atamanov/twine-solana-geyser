      "schemaVersion": 1
    },
    "database-operations": {
      "name": "Database Operations",
      "model": {
        "datasource": {
          "type": "postgres",
          "uid": "${postgres}"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "",
