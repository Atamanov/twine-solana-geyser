    "active-resources": {
      "name": "Active Resources",
      "model": {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            },
            "unit": "none"
          },
          "overrides": []
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "editorMode": "code",
            "expr": "geyser_active_slots",
            "legendFormat": "Active Slots",
            "range": true,
            "refId": "A"
          }
        ],
        "title": "Active Slots",
        "type": "gauge"
      },
      "type": "gauge",
      "schemaVersion": 1
    },
    "latency-histogram": {
      "name": "Operation Latencies",
      "model": {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
