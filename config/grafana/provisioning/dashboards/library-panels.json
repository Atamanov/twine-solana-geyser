          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "editorMode": "code",
            "expr": "histogram_quantile(0.50, sum(rate(geyser_write_latency_microseconds_bucket[5m])) by (le, operation))",
            "legendFormat": "p50 {{operation}}",
            "range": true,
            "refId": "C"
          }
        ],
